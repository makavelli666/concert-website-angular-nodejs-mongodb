CREATE TABLE IF NOT EXISTS evenements (
    id INTEGER PRIMARY KEY,
    nom TEXT,
    dateEcheance TEXT,
    priorite TEXT,
    statut TEXT,
    commentaire TEXT,
    prixBillets REAL,
    genreEvenement TEXT,
    image TEXT
);

CREATE TABLE IF NOT EXISTS artistes(
    id INTEGER PRIMARY KEY,
    evenementId INTEGER,
    artiste TEXT,
    FOREIGN KEY(evenementId) REFERENCES evenements(id)
);

